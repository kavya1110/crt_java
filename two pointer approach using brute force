import java.util.*;
class Solution
 {
    public static void main(String[] args) 
    {
        Scanner k=new Scanner(System.in);
        int n=k.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=k.nextInt();
        }
   int t=k.nextint();
   for(int i=0;i<n-1;i++)
{
for(int j=i+1;j<n;j++)
{
    if(a[i]+a[j]==t)
{
System.out.println(i+" "+j);
return;
}
}
    }
    System.out.println("-1");
}
}
