import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
       
        Scanner k=new Scanner(System.in);
         long A,B;
        A=k.nextLong();
        B=k.nextLong();
        long max=Math.max(A,B);
        System.out.println(max);
        k.close();
         
        
    }
    }
